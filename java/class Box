class Box
{
	double width,w;
	double height,h;
	double depth,d;
	void setdim()
	{
	w=width;
	h=height;
	d=depth;
	}
	double volume()
	{
	return w*h*d;
	}
}
class Boxdemo
{
	public static void main(String args[])
	double volume;
	{
	Box b1=new Box();
	Box b2=new Box();
	b1.width=2;
	b1.height=5;
	b1.setdim();
	vol=b1.volume();
	}
}